    text-decoration: underline !important;
}

#textbanner a.textbanner-link {
    color: #fff !important;
    text-decoration: none !important;
}
#textbanner a.textbanner-link:hover {
    color: #fff !important;
    text-decoration: underline !important;
}

div.list-add-to-cart button.add-to-cart {
    width: 170px;
    padding: .2rem 0;
    font-size: 0.8rem;
    height:35px;
    background-color: white;
    color: #d73672;
    border: 1px solid #ccc;
}

div.list-add-to-cart button.add-to-cart:hover {
    background-color: #f766a2;
    border: 1px solid #f766a2;
}


div.list-add-to-cart .product-quantity .add {
    margin-left: 0;
}


.product-miniature .thumbnail-container {
    height: 400px !important;
}

.product-miniature .product-description {
    bottom: 80px !important;
}

div.list-add-to-cart .product-quantity .btn-touchspin {
    height: 18px;
}

div.list-add-to-cart {
    margin-top: -20px;
}

div.list-add-to-cart .quantity_wanted {
    height:35px;
}


/* for use in product desc etc */
div.product-information h6 {
    color:#2fb5d2;
    font-weight: normal;
    line-height: 1.4;
    font-size: 0.85rem;
    margin-top: 0.5rem;
    margin-bottom: 0.5rem;

}


.product-information h2,
.page-content.page-cms h2 {
    color: #d73672;
    font-weight: bold;
    font-size: .9375rem;
}
.product-information h3,
.page-content.page-cms h3 {
    color:#2fb5d2;
    font-weight: bold;
    font-size: .9375rem;
}

.product-information h4,
.page-content.page-cms h4 {
    color: #232323;;
    font-weight: bold;
    font-size: .9375rem;
}

#notifications {
    margin-top: 144px;
    display: block !important;
}
@media (min-width: 768px){
    #notifications {
        margin-top: 20px;
    }
}